#!/bin/sh 

TESTNAME=$1
OUTFILE=$2
TMPFILE=$OUTFILE.prediff.tmp

# remove warning for ref-maybe-const deprecation
# TODO: once the optimization is fixed, this should be deleted
sed "/warning: inferring a default intent to be 'ref' is deprecated - please add an explicit 'ref' forall intent for/d" < $OUTFILE > $TMPFILE
cat $TMPFILE > $OUTFILE

rm $TMPFILE
