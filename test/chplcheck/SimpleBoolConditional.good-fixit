module SimpleBoolConditional {
  proc foo(x) {
    return x /**/ == 1;
  }
  proc fooInv(x) {
    return !(x == 1);
  }
  proc bar(x) {
    return x == /**/1
  }
  proc barInv(x) {
    return !(x == 1)
  }
  proc egg(x) {
    return x == 1
  }
  proc eggInv(x) {
    return !(x /**/== 1)
  }
  proc spam(x) {
    var a = x == 1;
    var b = !(x == 2);

    const c = x == 3,
          d = !(x == 4);
    return a && b && c && d;
  }
}
