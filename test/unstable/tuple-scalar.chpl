
var tuple = (10, 20, 30);
var scalar = 5;

writeln(tuple  + scalar);
writeln(scalar + tuple);
writeln(tuple  - scalar);
writeln(scalar - tuple);
writeln(tuple  * scalar);
writeln(scalar * tuple);
writeln(tuple  / scalar);
writeln(scalar / tuple);
writeln(tuple  % scalar);
writeln(scalar % tuple);
writeln(tuple  ** scalar);
writeln(scalar ** tuple);
writeln(tuple  & scalar);
writeln(scalar & tuple);
writeln(tuple  | scalar);
writeln(scalar | tuple);
writeln(tuple  ^ scalar);
writeln(scalar ^ tuple);
writeln(tuple  << scalar);
writeln(scalar << tuple);
writeln(tuple  >> scalar);
writeln(scalar >> tuple);
