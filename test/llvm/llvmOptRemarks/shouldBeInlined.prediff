#!/bin/sh

TESTNAME=$1
OUTFILE=$2
TMPFILE=$OUTFILE.prediff.tmp

# filter out line numbers
sed -E 's/\.chpl:[0-9]*:/\.chpl:n:/' < $OUTFILE > $TMPFILE
cat $TMPFILE > $OUTFILE

# filter out any line that doesn't refer to 'foo_chpl'
grep 'foo_chpl' < $OUTFILE > $TMPFILE
cat $TMPFILE > $OUTFILE

rm $TMPFILE
